   "source": [
    "def evcl(a,b):\n",
    "    while a!=0 and b!=0:\n",
    "        if a>b:\n",
    "            a=a%b\n",
    "        else:\n",
    "            b=b%a\n",
    "    return max(a,b)\n",
    "\n",
    "num1=int(input(\"Введите первое число:\"))\n",
    "num2=int(input(\"Введите второе число:\"))\n",
    "nod=evcl(num1,num2)\n",
    "print(\"Наибольший общий делитель\", nod)\n"
   ]
